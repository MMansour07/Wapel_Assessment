                    <!--end::Item-->
                </ul>
            </div>
            <!--end::Toolbar-->
        </div>
        <div class="card-body px-0">
                <div class="tab-content">
                    <!--begin::Tab-->
                    <div class="tab-pane show active px-7" id="kt_user_edit_tab_1" role="tabpanel">
                        <!--begin::Row-->
                        <div class="row">
                            <div class="col-xl-2"></div>
                            <div class="col-xl-7 my-2">
                                <div class="row">
                                    <div class="col-xl-12">
                                        <div class="form-group">
                                            @Html.ValidationSummary(true, "", new { @class = "text-danger", @readonly = "readonly"})
                                        </div>
                                    </div>
                                </div>

                                <div class="form-group row">
                                    <div class="col-6">
                                        <label class="col-form-label text-lg-right text-left">Task Criteria</label>
                                        @Html.DropDownListFor(model => model.TaskCriteriaId, new SelectList(ViewBag?.Criterias, "Id", "Name"), new { @class = "form-control form-control-lg form-control-solid", @id = "kt_select2", @name = "kt_select2", @disabled = "disabled" })
                                    </div>
                                    <div class="col-6">
                                        <label class="col-form-label text-lg-right text-left">Task</label>
                                        @Html.DropDownListFor(model => model.TaskId, new SelectList(ViewBag?.Tasks, "Id", "Name"), new { @class = "form-control form-control-lg form-control-solid", @id = "kt_select", @name = "kt_select", @disabled = "disabled" })
                                    </div>
                                </div>

                                <div class="form-group row">
                                    <div class="col-6">
                                        <label class="col-form-label text-lg-right text-left">Quality Score</label>
                                        @Html.TextBoxFor(model => model.QualityScore, new { @class = "form-control form-control-lg form-control-solid", @placeholder = "Quality Score", @name = "QualityScore", @type = "number", @readonly = "readonly"})
                                    </div>
                                    <div class="col-6">
                                        <label class="col-form-label text-lg-right text-left">Efficiency Score</label>
                                        @Html.TextBoxFor(model => model.EfficiencyScore, new { @class = "form-control form-control-lg form-control-solid", @placeholder = "Efficiency Score", @name = "EfficiencyScore", @type = "number", @readonly = "readonly"})
                                    </div>
                                </div>


                                <div class="form-group row">
                                    <div class="col-6">
                                        <label class="col-form-label text-lg-right text-left">Accuracy Score</label>
                                        @Html.TextBoxFor(model => model.AccuracyScore, new { @class = "form-control form-control-lg form-control-solid", @placeholder = "Accuracy Score", @name = "AccuracyScore", @type = "number", @readonly = "readonly"})
                                    </div>
                                    <div class="col-6">
                                        <label class="col-form-label text-lg-right text-left">Timeline Score</label>
                                        @Html.TextBoxFor(model => model.TimelineScore, new { @class = "form-control form-control-lg form-control-solid", @placeholder = "Timeline Score", @name = "TimelineScore", @type = "number", @readonly = "readonly"})
                                    </div>
                                </div>

                                <div class="form-group row">
                                    <div class="col-6">
                                        <label class="col-form-label text-lg-right text-left">Remarks</label>
                                        @Html.TextAreaFor(model => model.Remarks, new { @class = "form-control form-control-lg form-control-solid", @placeholder = "Remarks", @name = "Remarks", @readonly = "readonly"})
                                    </div>
                                </div>



                                <div class="separator separator-solid"></div>
                                <!--begin::Items-->
                                <div class="d-flex align-items-center flex-wrap mt-8">
                                    <div class="my-lg-0  m-auto my-3">
                                        <a href="@Url.Action("Index", "Rating")" type="reset" class="btn btn-secondary">Back</a>
                                    </div>
                                </div>
                                <!--begin::Items-->

                            </div>
                        </div>
                        <!--end::Row-->
                    </div>
                    <!--end::Tab-->
                </div>
        </div>
    </div>
</div>


<script src="~/Content/assets/js/pages/custom/Rating/newrating.js"></script>